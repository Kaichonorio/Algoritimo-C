#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n1,n2;
    printf("\nInforme o numero x: ");
    scanf("%d", &n1);

    printf("\nInforme o numero y: ");
    scanf("%d", &n2);

    if (n1 < n2){
        printf("\nEm ordem: %d, %d",n1, n2);
    }
    else if(n2 < n1){
        printf("\nEm ordem: %d, %d",n2, n1);
    }
    else
        printf("\nNumeros Iguais");

}
